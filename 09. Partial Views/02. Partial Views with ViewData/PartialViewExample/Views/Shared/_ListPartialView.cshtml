
@* We can use our own ViewData here in partial view *@

@{
    ViewData["ListTitle"] = "Updated";
}

<div class="list-container">
    <h3>@ViewData["ListTitle"]</h3>
    <ul class="list">
        @foreach (string item in ViewBag.ListItems)
        {
            <li>@item</li>
        }
    </ul>
</div>